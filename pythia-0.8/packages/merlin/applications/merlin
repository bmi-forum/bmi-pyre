# main
if __name__ == '__main__':

    import journal

    journal.info('merlin').activate()
    journal.debug('merlin').activate()

    journal.info('merlin.curator').activate()
    journal.debug('merlin.curator').activate()
    
    import merlin
    app = merlin.merlin()
    app.run()


# version
__id__ = "$Id: merlin,v 1.1.1.1 2005/03/08 16:14:00 aivazis Exp $"

# End of file 
